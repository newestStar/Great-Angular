{
  "name": "great-big-angular2-example",
  "version": "0.0.0",
  "description": "A fullstack, multi-feature Angular2/Redux app made from several single-feature apps",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "start": "ng serve --proxy-config proxy.conf.json",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "xbackend": "json-server --watch server/db/db.json --port 3001",
    "xstart": "cross-env NODE_ENV=production node server/node-server.js",
    "xlint": "npm run lint-ts && npm run lint-js && npm run lint-css",
    "xtest": "karma start --singleRun true",
    "xpree2e": "webdriver-manager update",
    "xe2e": "protractor",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack",
    "clean": "rimraf dist/",
    "cover": "istanbul report --include=**/chrome/coverage-final.json text text-summary",
    "dev:server": "cross-env NODE_ENV=development PORT=3000 nodemon server/node-server.js",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server -d --inline --progress --no-info",
    "dev": "concurrently -r \"npm run dev:client\" \"npm run dev:server\"",
    "lint-css": "stylelint './src/**/*.css'",
    "lint-ts": "tslint './src/**/*.ts'",
    "lint-js": "eslint '**/*.js' --ignore-path .gitignore",
    "xpostinstall": "npm run typings && npm run build",
    "pretest": "rimraf coverage/ & npm run lint",
    "posttest": "npm run cover",
    "test:watch": "karma start --singleRun false",
    "typings": "rimraf typings/ && typings install"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^2.1.0",
    "@angular/compiler": "^2.1.0",
    "@angular/core": "^2.1.0",
    "@angular/forms": "^2.1.0",
    "@angular/http": "^2.1.0",
    "@angular/material": "^2.0.0-alpha.9-3",
    "@angular/platform-browser": "^2.1.0",
    "@angular/platform-browser-dynamic": "^2.1.0",
    "@angular/router": "^3.0.0",
    "@ngrx/core": "^1.0.0",
    "@ngrx/db": "^2.0.0",
    "@ngrx/effects": "^2.0.0",
    "@ngrx/router-store": "^1.1.0",
    "@ngrx/store": "^2.0.0",
    "@ngrx/store-devtools": "^3.1.0",
    "@types/hammerjs": "2.0.33",
    "angular-sortablejs": "0.1.0",
    "autoprefixer": "^6.3.3",
    "autoprefixer-core": "^6.0.1",
    "awesome-typescript-loader": "^2.2.1",
    "babel-core": "^6.11.4",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.9.0",
    "basscss": "^8.0.1",
    "basscss-addons": "^1.0.0-beta4",
    "basscss-basic": "^1.0.0",
    "concurrently": "^2.0.0",
    "copy-webpack-plugin": "^3.0.1",
    "core-js": "^2.4.1",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
